      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "virtualpytest-postgres"
          },
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT \n  c.name as \"Campaign\",\n  COUNT(te.id) as \"Total Tests\",\n  COUNT(CASE WHEN te.status = 'completed' THEN 1 END) as \"Completed\",\n  COUNT(CASE WHEN te.status = 'failed' THEN 1 END) as \"Failed\",\n  ROUND(\n    COUNT(CASE WHEN te.status = 'completed' THEN 1 END)::numeric / \n    NULLIF(COUNT(te.id), 0) * 100, 2\n  ) as \"Success Rate %\"\nFROM campaigns c\nLEFT JOIN test_executions te ON c.id = te.campaign_id\nWHERE c.created_at >= NOW() - INTERVAL '7 days'\nGROUP BY c.id, c.name\nORDER BY \"Success Rate %\" DESC\nLIMIT 10",
          "refId": "A",
          "select": [
            [
              {
                "params": ["*"],
                "type": "column"
              }
            ]
          ],
          "table": "campaigns",
          "timeColumn": "created_at",
          "timeColumnType": "timestamp",
          "where": []
        }
      ],
      "title": "Campaign Performance (Last 7 Days)",
      "type": "table"
