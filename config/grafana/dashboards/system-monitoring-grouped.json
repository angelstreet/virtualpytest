              "rawQuery": true,
              "rawSql": "SELECT \n    'server' as \"Server\",\n    TO_CHAR(MAX(timestamp), 'YYYY-MM-DD HH24:MI:SS') as \"Last Update\",\n    ROUND(EXTRACT(EPOCH FROM (NOW() - MAX(timestamp)))/3600, 2) as \"Uptime (hours)\",\n    COUNT(*) as \"Uptime (seconds)\",\n    ROUND(COUNT(*) * 100.0 / EXTRACT(EPOCH FROM (NOW() - MIN(timestamp))), 2) as \"Uptime (%)\",\n    COUNT(*) as \"Received Pings\",\n    ROUND(EXTRACT(EPOCH FROM (NOW() - MIN(timestamp)))/60, 0) as \"Expected Pings\",\n    ROUND(COUNT(*) * 100.0 / (EXTRACT(EPOCH FROM (NOW() - MIN(timestamp)))/60), 2) as \"Daily % (24h)\",\n    CASE \n        WHEN COUNT(*) * 100.0 / (EXTRACT(EPOCH FROM (NOW() - MIN(timestamp)))/60) >= 95 THEN 'Good'\n        WHEN COUNT(*) * 100.0 / (EXTRACT(EPOCH FROM (NOW() - MIN(timestamp)))/60) >= 90 THEN 'Warning'\n        ELSE 'Critical'\n    END as \"Daily Downtime (h)\",\n    CASE \n        WHEN COUNT(*) * 100.0 / (EXTRACT(EPOCH FROM (NOW() - MIN(timestamp)))/60) >= 95 THEN 'Good'\n        WHEN COUNT(*) * 100.0 / (EXTRACT(EPOCH FROM (NOW() - MIN(timestamp)))/60) >= 90 THEN 'Warning'\n        ELSE 'Critical'\n    END as \"Weekly % (7d)\",\n    CASE \n        WHEN COUNT(*) * 100.0 / (EXTRACT(EPOCH FROM (NOW() - MIN(timestamp)))/60) >= 95 THEN 'Good'\n        WHEN COUNT(*) * 100.0 / (EXTRACT(EPOCH FROM (NOW() - MIN(timestamp)))/60) >= 90 THEN 'Warning'\n        ELSE 'Critical'\n    END as \"Weekly Downtime (h)\"\nFROM system_metrics \nWHERE host_name = 'server'\n  AND timestamp >= NOW() - INTERVAL '24 hours'",
