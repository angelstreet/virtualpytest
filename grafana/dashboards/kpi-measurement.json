          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT er.action_set_id, AVG(er.kpi_measurement_ms) as avg_kpi, (SELECT kpi_report_url FROM execution_results er2 LEFT JOIN navigation_trees nt2 ON er2.tree_id = nt2.id LEFT JOIN userinterfaces ui2 ON nt2.userinterface_id = ui2.id WHERE er2.action_set_id = er.action_set_id AND ui2.name = 'horizon_android_tv' AND er2.kpi_report_url IS NOT NULL AND $__timeFilter(er2.executed_at) ORDER BY er2.executed_at DESC LIMIT 1) as report_url FROM execution_results er LEFT JOIN navigation_trees nt ON er.tree_id = nt.id LEFT JOIN userinterfaces ui ON nt.userinterface_id = ui.id WHERE er.kpi_measurement_ms IS NOT NULL AND ui.name = 'horizon_android_tv' AND $__timeFilter(er.executed_at) GROUP BY er.action_set_id ORDER BY avg_kpi DESC",
          "refId": "A"
        }
      ],
      "title": "KPI by Action Set - horizon_android_tv",
      "type": "barchart"
    },
