          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  userinterface_name as \"User Interface\",\n  ROUND(AVG(blackscreen_freeze_duration_seconds * 1000)::numeric, 2) as \"Mean Blackscreen Duration\"\nFROM zap_results \nWHERE $__timeFilter(created_at)\n  AND userinterface_name IS NOT NULL\n  AND userinterface_name != ''\n  AND blackscreen_freeze_duration_seconds IS NOT NULL\nGROUP BY userinterface_name\nORDER BY \"Mean Blackscreen Duration\" DESC",
          "refId": "A",
          "sql": {
            "columns": [],
            "groupBy": [],
            "limit": 50
          }
        }
      ],
      "title": "Mean Blackscreen Duration by User Interface",
