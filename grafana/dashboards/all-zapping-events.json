      "targets": [
        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "supabase-postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  created_at as \"Detected At\",\n  device_name as \"Device\",\n  userinterface_name as \"Source\",\n  channel_name as \"Channel\",\n  program_name as \"Program\",\n  CONCAT(program_start_time, '-', program_end_time) as \"Program Time\",\n  detection_method as \"Detection\",\n  COALESCE(action_params->>'key', action_command) as \"Action\",\n  ROUND((blackscreen_freeze_duration_seconds * 1000)::numeric, 0) as \"BS Duration (ms)\",\n  time_since_action_ms as \"Action Delay (ms)\",\n  total_zap_duration_ms as \"Total Zap (ms)\",\n  ROUND(audio_silence_duration::numeric, 2) as \"Audio Silence (s)\"\nFROM zap_results \nWHERE $__timeFilter(created_at)\n  AND channel_name IS NOT NULL\n  AND channel_name != ''\nORDER BY created_at DESC\nLIMIT 200",
          "refId": "A",
          "sql": {
            "columns": [],
            "groupBy": [],
            "limit": 50
          },
          "table": "zap_results"
        }
