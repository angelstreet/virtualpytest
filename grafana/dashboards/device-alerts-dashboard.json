        "targets": [
          {
            "datasource": {
              "uid": "ceunuuxwxvy80e"
            },
            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT \n    host_name || '-' || device_name as \"metric\",\n    CASE \n        WHEN active_count > 0 THEN 1\n        ELSE 0\n    END as \"value\"\nFROM (\n    SELECT \n        all_devices.host_name,\n        all_devices.device_name,\n        COALESCE(active_alerts.alert_count, 0) as active_count\n    FROM (\n        SELECT DISTINCT host_name, device_name \n        FROM alerts \n        WHERE device_name IS NOT NULL \n          AND host_name IS NOT NULL\n    ) all_devices\n    LEFT JOIN (\n        SELECT host_name, device_name, COUNT(*) as alert_count\n        FROM alerts \n        WHERE status = 'active'\n        GROUP BY host_name, device_name\n    ) active_alerts ON (\n        all_devices.host_name = active_alerts.host_name \n        AND all_devices.device_name = active_alerts.device_name\n    )\n) device_status\nORDER BY host_name, device_name",
            "refId": "A"
          }
        ],
        "title": "\ud83d\udea8 Device Alert Status",
        "type": "stat"
      },
