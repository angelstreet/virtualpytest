            "editorMode": "code",
            "format": "time_series",
            "rawQuery": true,
            "rawSql": "WITH time_buckets AS (\n  SELECT generate_series(\n    date_trunc('minute', $__timeFrom()::timestamp),\n    date_trunc('minute', $__timeTo()::timestamp),\n    '1 minute'::interval\n  ) AS time\n),\nall_devices AS (\n  SELECT DISTINCT host_name, device_name\n  FROM alerts\n  WHERE device_name IS NOT NULL\n    AND host_name IS NOT NULL\n    AND start_time >= $__timeFrom()::timestamp - INTERVAL '1 hour'\n),\nincident_counts AS (\n  SELECT\n    tb.time,\n    ad.host_name,\n    ad.device_name,\n    COUNT(a.id) as total_count\n  FROM time_buckets tb\n  CROSS JOIN all_devices ad\n  LEFT JOIN alerts a ON (\n    a.host_name = ad.host_name\n    AND a.device_name = ad.device_name\n    AND a.start_time <= tb.time\n    AND (a.end_time IS NULL OR a.end_time > tb.time)\n  )\n  GROUP BY tb.time, ad.host_name, ad.device_name\n)\nSELECT\n  ic.time,\n  ic.host_name || '-' || ic.device_name as metric,\n  ic.total_count as value\nFROM incident_counts ic\nORDER BY ic.time, ic.host_name, ic.device_name",
            "refId": "A",
