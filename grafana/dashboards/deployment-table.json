            "rawQuery": true,
            "rawSql": "SELECT \n  CONCAT(host_name, '-', device_name) as device,\n  script_name as task,\n  started_at as start,\n  CASE WHEN completed_at - started_at < '1 minute'::interval THEN started_at + '1 minute'::interval ELSE completed_at END as end,\n  success\nFROM script_results \nWHERE started_at >= NOW() - INTERVAL '24 hours'\n  AND ('${user_interface:csv}' = '' OR userinterface_name IN ($user_interface))\n  AND ('${host:csv}' = '' OR host_name IN ($host))\n  AND ('${device_name:csv}' = '' OR device_name IN ($device_name))\nORDER BY device, started_at\nLIMIT 100",
