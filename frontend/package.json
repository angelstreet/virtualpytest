{
  "name": "virtualpytest-web",
  "version": "1.0.0",
  "description": "Web interface for VirtualPyTest framework",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run copy-docs && tsc && vite build",
